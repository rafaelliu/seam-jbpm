      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd"
      login-required="true">

	<navigation>
		<rule if-outcome="submeter">
			<begin-task/>
			<redirect view-id="/submicaoComunicado.xhtml" />
		</rule>
		<rule if-outcome="aprovacao">
			<begin-task/>
